<mapping
    xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
    xmlns='http://www.sitoolkit.org/schema/tabledata-mapping'
    xsi:schemaLocation='http://www.sitoolkit.org/schema/tabledata-mapping https://raw.githubusercontent.com/sitoolkit/sit-util-td/master/src/main/xsd/tabledata-mapping-1.0.xsd'>

    <table beanId="testStep" name="テストステップ">
        <column property="no" name="No."/>
        <column property="itemName" name="項目名"/>
        <column property="operation" name="操作"/>
        <column property="locator.type" name="ロケーター形式"/>
        <column property="locator.value" name="ロケーター"/>
        <column property="dataType" name="データ形式"/>
        <column property="screenshotTiming" name="スクリーンショット"/>
        <column property="testData" name="テストデータ" pattern="ケース_(.*)$"/>
        <column property="breakPoint" name="ブレークポイント"/>
    </table>

    <table beanId="pageItemSpec" name="画面定義書ファイル">
        <column property="name" name="項目"/>
        <column property="control" name="コントロール"/>
        <column property="pname" name="項目物理名"/>
    </table>

</mapping>
